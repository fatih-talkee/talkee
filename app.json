{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "net.talkee.app",
      "supportsTablet": true,
      "googleServicesFile": "./firebase/ios/GoogleService-Info.plist",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs microphone access for calls.",
        "NSCameraUsageDescription": "This app needs camera access for video calls.",
        "UIBackgroundModes": ["audio", "voip"],
        "AVAudioSessionCategory": "PlayAndRecord"
      }
    },
    "android": {
      "package": "net.talkee.app",
      "googleServicesFile": "./firebase/android/google-services.json",
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA",
        "BLUETOOTH",
        "BLUETOOTH_CONNECT",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "usesCleartextTraffic": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "firebase": {
        "apiKey": "AIzaSyDEfmC53rDYrwAJdL-HwB-sYJ4_h_HWnA0",
        "authDomain": "talkee-d41d6.firebaseapp.com",
        "projectId": "talkee-d41d6",
        "storageBucket": "talkee-d41d6.firebasestorage.app",
        "messagingSenderId": "968951986533",
        "appId": "1:968951986533:web:3f3ac36597a2184cad898c",
        "measurementId": "G-QETSVPW4F7"
      },
      "eas": {
        "projectId": "dfd312c3-0e23-42cb-b37e-5cc26f629eb2"
      }
    }
  }
}
